@model string
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row" ng-init="courses = @Html.Raw(Model)">
    <div class="col-md-10">
        <table class="table table-condensed table-hover">
            <tr>
                <th>Course</th>
                <th>Course Name</th>
                <th>Instructor</th>
            </tr>
            <tr ng-repeat="course in courses">
                <td>{{course.number}}</td>
                <td>{{course.name}}</td>
                <td>{{course.instructor}}</td>
            </tr>
        </table>
    </div>
</div>



